{
  "name": "nz-schema-form",
  "version": "1.0.0-beta.0",
  "license": "MIT",
  "description": "ng-zorro-antd form generation based on JSON-Schema",
  "main": "./bundles/index.js",
  "author": "cipchk",
  "homepage": "https://cipchk.github.io/nz-schema-form/",
  "bugs": {
    "url": "https://github.com/cipchk/nz-schema-form/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run release:validity",
    "test": "ng test -sr -cc",
    "lint": "tslint -p tsconfig.json -c tslint.json 'lib/**/*.ts' -e 'lib/**/*.spec.ts'",
    "e2e": "ng e2e",
    "demo.gh-pages": "ng build --prod --build-optimizer && gh-pages -d dist",
    "clean:tmp": "rimraf .ng_build .ng_compiled",
    "clean:tmp:lib": "npm run clean:tmp && rimraf .lib",
    "build:copy-sources": "gulp copy-sources",
    "build:inline-resources": "gulp inline-resources",
    "build:ts": "tsc -p tsconfig.publish.json && ngc -p tsconfig.publish.json",
    "build:bundle": "gulp bundle",
    "build:package": "npm-run-all -s clean:tmp:lib build:copy-sources build:ts build:inline-resources build:bundle clean:tmp",
    "version:bump": "gulp bump",
    "release:prepare": "npm run build:package",
    "release:validity": "npm run version:bump && npm run release:prepare",
    "release": "npm run version:bump && npm run release:prepare && npm publish --access=public .lib --tag next"
  },
  "peerDependencies": {
    "z-schema": "^3.24.1",
    "@types/z-schema": "^3.16.32",
    "ng-zorro-antd": "^1.8.0"
  },
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/router": "^7.0.2",
    "core-js": "^2.5.7",
    "ng-zorro-antd": "^1.8.0",
    "ng2-ace-editor": "^0.3.8",
    "ngx-highlight-js": "^2.1.1",
    "ngx-markdown": "^7.0.0",
    "ngx-md": "^7.0.0",
    "ngx-tinymce": "^2.1.1",
    "ngx-ueditor": "^2.1.3",
    "rxjs": "^6.3.3",
    "z-schema": "^3.24.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^7.0.3",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.1",
    "@types/z-schema": "^3.16.32",
    "codecov": "^3.1.0",
    "codelyzer": "^4.5.0",
    "gulp": "^4.0.0",
    "gulp-bump": "^3.1.1",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-cli": "^2.0.1",
    "gulp-connect": "^5.6.1",
    "gulp-dom": "^1.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-if": "^2.0.2",
    "gulp-less": "^4.0.1",
    "gulp-markdown": "^3.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rollup": "^2.16.2",
    "gulp-transform": "^3.0.5",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.5"
  }
}
